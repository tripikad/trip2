{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "nikic/php-parser": "2.0.0",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "laravelcollective/html": "5.3.*@dev",
        "intervention/image": "^2.3@dev",
        "barryvdh/laravel-debugbar": "^2.0@dev",
        "baum/baum": "~1.1",
        "predis/predis": "~1.0",
        "wpb/string-blade-compiler": "2.*@dev",
        "jenssegers/date": "3.*@dev",
        "ipunkt/laravel-analytics": "~1.1",
        "roumen/feed": "~2.10",
        "laravel/socialite" : "~2.0",
        "laravelista/ekko": "~1.0",
        "graham-campbell/htmlmin": "^4.3",
        "cviebrock/eloquent-sluggable": "^4.0",
        "doctrine/dbal": "^2.5",
        "msurguy/honeypot": "^1.0",
        "roumen/sitemap": "^2.6",
        "andreasindal/laravel-markdown": "^1.1",
        "laravel/scout": "2.0",
        "teamtnt/laravel-scout-tntsearch-driver": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "phpspec/phpspec": "~2.1",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Utils/utils.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
